<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
<interface name="org.mauiman.Accessibility">

<method name="setSingleClick">
<arg name="singleClick" type="b" direction="in"/>
</method>

<method name="setScrollBarPolicy">
<arg name="scrollBarPolicy" type="u" direction="in"/>
</method>

<method name="setPlaySounds">
<arg name="playSounds" type="b" direction="in"/>
</method>

<property name="singleClick" type="b" access="read"/>
<property name="scrollBarPolicy" type="u" access="read"/>
<property name="playSounds" type="b" access="read"/>

<signal name="singleClickChanged">
<arg type="b"/>
</signal>

<signal name="scrollBarPolicyChanged">
<arg type="u"/>
</signal>

<signal name="playSoundsChanged">
<arg type="b"/>
</signal>

</interface>
</node>
