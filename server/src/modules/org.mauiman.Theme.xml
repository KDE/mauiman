<signal name="accentColorChanged">
<arg type="s"/>
</signal>
<signal name="styleTypeChanged">
<arg type="i"/>
</signal>
<signal name="iconThemeChanged">
<arg type="s"/>
</signal>
<signal name="windowControlsThemeChanged">
<arg type="s"/>
</signal>
<signal name="enableCSDChanged">
<arg type="b"/>
</signal>
<signal name="borderRadiusChanged">
<arg type="u"/>
</signal>
<signal name="iconSizeChanged">
<arg type="u"/>
</signal>
<signal name="paddingSizeChanged">
<arg type="u"/>
</signal>
<signal name="marginSizeChanged">
<arg type="u"/>
</signal>
<signal name="spacingSizeChanged">
<arg type="u"/>
</signal>
<signal name="enableEffectsChanged">
<arg type="b"/>
</signal>
</interface>
</node>
